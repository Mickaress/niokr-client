    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vue3-toastify": "^0.2.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.4",
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-essentials": "^8.0.10",
